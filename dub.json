{
	"name": "dqt",
	"description": "D bindings for Qt.",
	"authors": ["Tim Schendekehl"],
	"license": "LGPL-3.0",
	"targetType": "none",
	"dependencies": {
		"dqt:widgets": "*",
	},
	"subPackages": [
		{
			"name": "core",
			"targetType": "library",
			"sourcePaths": ["core"],
			"importPaths": ["core"],
			"libs-windows": ["Qt6Core"],
			"libs-linux": ["Qt6Core", "stdc++"],
			"libs-osx": ["c++"],
			"lflags-osx": ["-framework", "QtCore"],
			"toolchainRequirements": {
				"frontend": ">=2.103"
			}
		},
		{
			"name": "gui",
			"targetType": "library",
			"sourcePaths": ["gui"],
			"importPaths": ["gui"],
			"libs-linux": ["Qt6Gui"],
			"libs-windows": ["Qt6Gui"],
			"lflags-osx": ["-framework", "QtGui"],
			"dependencies": {"dqt:core":"*"}
		},
		{
			"name": "widgets",
			"targetType": "library",
			"sourcePaths": ["widgets"],
			"importPaths": ["widgets"],
			"libs-linux": ["Qt6Widgets"],
			"libs-windows": ["Qt6Widgets"],
			"lflags-osx": ["-framework", "QtWidgets"],
			"dependencies": {"dqt:gui":"*", "dxml": "~>0.4.4"}
		},
		{
			"name": "network",
			"targetType": "library",
			"sourcePaths": ["network"],
			"importPaths": ["network"],
			"libs-linux": ["Qt6Network"],
			"libs-windows": ["Qt6Network"],
			"lflags-osx": ["-framework", "QtNetwork"],
			"dependencies": {"dqt:core":"*"}
		},
		{
			"name": "qml",
			"targetType": "library",
			"sourcePaths": ["qml"],
			"importPaths": ["qml"],
			"libs-linux": ["Qt6Qml"],
			"libs-windows": ["Qt6Qml"],
			"lflags-osx": ["-framework", "QtQml"],
			"dependencies": {"dqt:gui":"*"}
		},
		{
			"name": "quick",
			"targetType": "library",
			"sourcePaths": ["quick"],
			"importPaths": ["quick"],
			"libs-linux": ["Qt6Quick"],
			"libs-windows": ["Qt6Quick"],
			"lflags-osx": ["-framework", "QtQuick"],
			"dependencies": {"dqt:qml":"*"}
		},
		{
			"name": "quickcontrols2",
			"targetType": "library",
			"sourcePaths": ["quickcontrols2"],
			"importPaths": ["quickcontrols2"],
			"libs-linux": ["Qt6QuickControls2"],
			"libs-windows": ["Qt6QuickControls2"],
			"lflags-osx": ["-framework", "QtQuickControls2"],
			"dependencies": {"dqt:quick":"*"}
		},
		{
			"name": "webenginecore",
			"targetType": "library",
			"sourcePaths": ["webenginecore"],
			"importPaths": ["webenginecore"],
			"libs-linux": ["Qt6WebEngineCore"],
			"libs-windows": ["Qt6WebEngineCore"],
			"lflags-osx": ["-framework", "QtWebEngineCore"],
			"dependencies": {"dqt:gui":"*", "dqt:network":"*"}
		},
		{
			"name": "webenginewidgets",
			"targetType": "library",
			"sourcePaths": ["webenginewidgets"],
			"importPaths": ["webenginewidgets"],
			"libs-linux": ["Qt6WebEngineWidgets"],
			"libs-windows": ["Qt6WebEngineWidgets"],
			"lflags-osx": ["-framework", "QtWebEngineWidgets"],
			"dependencies": {"dqt:widgets":"*", "dqt:webenginecore":"*"}
		},
		{
			"name": "multimedia",
			"targetType": "library",
			"sourcePaths": ["multimedia"],
			"importPaths": ["multimedia"],
			"libs-linux": ["Qt6Multimedia"],
			"libs-windows": ["Qt6Multimedia"],
			"lflags-osx": ["-framework", "QtMultimedia"],
			"dependencies": {"dqt:gui":"*"}
		},
		{
			"name": "multimediawidgets",
			"targetType": "library",
			"sourcePaths": ["multimediawidgets"],
			"importPaths": ["multimediawidgets"],
			"libs-linux": ["Qt6MultimediaWidgets"],
			"libs-windows": ["Qt6MultimediaWidgets"],
			"lflags-osx": ["-framework", "QtMultimediaWidgets"],
			"dependencies": {"dqt:widgets":"*", "dqt:multimedia":"*"}
		},

		{
			"name": "helloworld",
			"targetType": "executable",
			"sourcePaths": ["examples/helloworld"],
			"importPaths": ["examples/helloworld"],
			"stringImportPaths": ["examples/helloworld"],
			"dependencies": {"dqt:widgets":"*"}
		},
		{
			"name": "examplewidgets",
			"targetType": "executable",
			"sourcePaths": ["examples/examplewidgets"],
			"importPaths": ["examples/examplewidgets"],
			"stringImportPaths": ["examples/examplewidgets"],
			"dependencies": {"dqt:widgets":"*"}
		},
		{
			"name": "exampleqml",
			"targetType": "executable",
			"sourcePaths": ["examples/exampleqml"],
			"importPaths": ["examples/exampleqml"],
			"stringImportPaths": ["examples/exampleqml"],
			"dependencies": {"dqt:quickcontrols2":"*"}
		},
		{
			"name": "examplebrowser",
			"targetType": "executable",
			"sourcePaths": ["examples/examplebrowser"],
			"importPaths": ["examples/examplebrowser"],
			"stringImportPaths": ["examples/examplebrowser"],
			"dependencies": {"dqt:webenginewidgets":"*"}
		},
		{
			"name": "mediaplayer",
			"targetType": "executable",
			"sourcePaths": ["examples/mediaplayer"],
			"importPaths": ["examples/mediaplayer"],
			"stringImportPaths": ["examples/mediaplayer"],
			"dependencies": {"dqt:multimediawidgets":"*"}
		}
	]
}
